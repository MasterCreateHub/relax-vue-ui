.theme-default-content:not(.custom){
    max-width: 100%;
}

table {
    display: table;
}

::-webkit-scrollbar {
    width: 12px; /* 滚动条宽度 */
}

::-webkit-scrollbar-track {
    background: #f1f1f1; /* 轨道背景颜色 */
}

::-webkit-scrollbar-thumb {
    background: #c1c1c1; /* 滑块颜色 */
    border-radius: 6px; /* 圆角 */
}

::-webkit-scrollbar-thumb:hover {
    background: #a8a8a8; /* 悬停时滑块颜色 */
}


